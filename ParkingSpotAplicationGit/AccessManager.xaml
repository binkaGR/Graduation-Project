<Window x:Class="ParkingSpotAplication.AccessManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParkingSpotAplication"
        mc:Ignorable="d"
        Title="AccessManager" Height="200" Width="200">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black"/>
                <GradientStop Color="#FF21EEE7" Offset="0.726"/>
                <GradientStop Color="#FFD8E6E5" Offset="0"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Button x:Name="ButttonParking" Content="Parking" HorizontalAlignment="Center" VerticalAlignment="Top" Height="49" Width="132" Margin="0,106,0,0" FontSize="16" Click="ButttonParking_Click" Background="#FFDEE01D"/>
        <Button x:Name="ButtonEmploy" Content="Employees" HorizontalAlignment="Center" VerticalAlignment="Top" Height="49" Width="132" Margin="0,22,0,0" FontSize="16" Click="ButtonEmploy_Click" Background="#FF59D21C"/>

    </Grid>
</Window>
